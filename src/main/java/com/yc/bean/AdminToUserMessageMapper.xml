<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.bean.AdminToUserMessageMapper">
	<!--加入带日志的ehcache缓存 -->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"></cache>
		
<!-- 添加借款详细信息 -->
<insert id="addMessageToUser" parameterType="AdminToUserMessage" useGeneratedKeys="true" keyProperty="atum_id">
insert into AdminToUserMessage(u_id,atum_time,atum_content,atum_status,atum_title) 
values(#{u_id},#{atum_time},#{atum_content},#{atum_status},#{atum_title}) 
</insert>





</mapper>